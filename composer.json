{
  "name": "simperfit/twilio-bundle",
  "type": "symfony-bundle",
  "description": "A simple Symfony 2, 3 bundle for the official sdk provided by Twilio.",
  "keywords": [
    "Twilio"
  ],
  "homepage": "https://github.com/fridolin-koch/VreshTwilioBundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Fridolin Koch",
      "email": "info@fridokoch.de"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.0",
    "symfony/framework-bundle": "~4.0|~5.0",
    "twilio/sdk": ">=4.0.2 <5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "symfony/yaml": "5.4.x-dev",
    "php-coveralls/php-coveralls": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Vresh\\TwilioBundle\\": ""
    }
  }
}
